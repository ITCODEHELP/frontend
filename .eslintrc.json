{
    "env": {
        "browser": true,
        "es2021": true
    },
    "root": true,
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "ignorePatterns": ["node_modules/"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecamFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "jsx-quotes": [
            "error",
            "prefer-double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-extra-semi": "warn",
        "react/react-in-jsx-scope": "off"
    }
}
